    }
    
    body {
      background: #0a0a0a;
      color: #fff;
    }
    
    .nav {
      position: fixed;
      top: 0;
      left: 0;
      right: 0;
      padding: 1rem 2rem;
      background: #0a0a0a;
      border-bottom: 1px solid #222;
      z-index: 100;
      display: flex;
      gap: 0.5rem;
      flex-wrap: wrap;
      align-items: center;
    }
    
    .nav-group {
      display: flex;
      gap: 0.5rem;
      flex-wrap: wrap;
    }
    
    .nav-divider {
      width: 1px;
      height: 24px;
      background: #333;
      margin: 0 0.5rem;
    }
    
    .nav a {
      color: #888;
      text-decoration: none;
      font-size: 0.75rem;
      padding: 0.4rem 0.8rem;
      border: 1px solid #333;
      border-radius: 4px;
      transition: all 0.2s;
      white-space: nowrap;
    }
    
    .nav a:hover {
      color: #fff;
      border-color: #fff;
    }
    
    .nav a.highlight {
      border-color: #4a9;
      color: #4a9;
    }
    
    .nav a.highlight:hover {
      background: #4a9;
      color: #000;
    }
    
    .nav .back {
      margin-left: auto;
    }
    
    .section {
      min-height: 100vh;
      padding: 5rem 2rem 4rem;
      border-bottom: 1px solid #222;
      position: relative;
    }
    
    .label {
      position: absolute;
      top: 5rem;
      left: 2rem;
      background: #1a1a1a;
      padding: 0.5rem 1rem;
      font-size: 0.75rem;
      letter-spacing: 0.1em;
      text-transform: uppercase;
      color: #666;
      font-family: system-ui;
    }
    
    .label.recommended {
      background: #1a2a1a;
      color: #4a9;
    }
    
    .hero {
      height: 50vh;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      background: linear-gradient(to bottom, #0a0a0a, #111);
      position: relative;
      margin-top: 2rem;
    }
    
    .hero h1 {
      font-size: clamp(2.5rem, 7vw, 5rem);
      letter-spacing: 0.12em;
      text-transform: uppercase;
      font-weight: 400;
      text-align: center;
    }
    
    .hero .tagline {
      margin-top: 1rem;
      font-size: 0.875rem;
      letter-spacing: 0.15em;
      text-transform: uppercase;
      color: rgba(255,255,255,0.5);
    }
    
    /* Language rows */
    .lang-rows {
      display: flex;
      flex-direction: column;
      gap: 0.5rem;
      margin-top: 2rem;
    }
    
    .lang-row {
      display: flex;
      align-items: center;
      gap: 1rem;
      font-size: 0.75rem;
      color: rgba(255,255,255,0.4);
    }
    
    .lang-label {
      width: 30px;
      text-transform: uppercase;
      letter-spacing: 0.1em;
      font-family: system-ui;
      font-size: 0.6rem;
    }
    
    .lang-text {
      font-size: 1.5rem;
      color: rgba(255,255,255,0.7);
      letter-spacing: 0.05em;
    }
    
    .about {
      max-width: 700px;
      margin: 0 auto;
      padding: 3rem 0;
    }
    
    .about h2 {
      font-size: 1.75rem;
      margin-bottom: 1.5rem;
      font-weight: 400;
      letter-spacing: 0.03em;
    }
    
    .about p {
      font-size: 1.05rem;
      line-height: 1.8;
      color: rgba(255,255,255,0.7);
      margin-bottom: 1.25rem;
    }
    
    .about strong {
      color: #fff;
      font-weight: 400;
    }
    
    .about em {
      color: #fff;
      font-style: normal;
    }
    
    /* Multi-language about */
    .about-multilang {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 2rem;
      max-width: 1200px;
      margin: 0 auto;
      padding: 3rem 0;
    }
    
    .about-col {
      padding: 1.5rem;
      background: rgba(255,255,255,0.02);
      border-radius: 8px;
    }
    
    .about-col .lang-tag {
      font-size: 0.65rem;
      letter-spacing: 0.15em;
      text-transform: uppercase;
      color: #555;
      margin-bottom: 1rem;
      font-family: system-ui;
    }
    
    .about-col h3 {
      font-size: 1.25rem;
      margin-bottom: 1rem;
      font-weight: 400;
    }
    
    .about-col p {
      font-size: 0.95rem;
      line-height: 1.7;
      color: rgba(255,255,255,0.65);
    }
    
    /* Font variants */
    
    /* 1. Elegant Classic */
    .font-1 h1, .font-1 h2, .font-1 h3 { font-family: 'Cormorant Garamond', serif; }
    .font-1 p, .font-1 .tagline, .font-1 .lang-text { font-family: 'Inter', sans-serif; font-weight: 300; }
    
    /* 2. Modern Mono */
    .font-2 h1, .font-2 h2, .font-2 h3 { font-family: 'JetBrains Mono', monospace; font-weight: 300; letter-spacing: 0.15em; }
    .font-2 p, .font-2 .tagline, .font-2 .lang-text { font-family: 'Crimson Text', serif; }
    
    /* 3. Editorial */
    .font-3 h1, .font-3 h2, .font-3 h3 { font-family: 'Playfair Display', serif; }
    .font-3 p, .font-3 .tagline, .font-3 .lang-text { font-family: 'Source Sans 3', sans-serif; font-weight: 300; }
    
    /* 4. Minimalist */
    .font-4 h1, .font-4 h2, .font-4 h3 { font-family: 'Space Grotesk', sans-serif; font-weight: 300; }
    .font-4 p, .font-4 .tagline, .font-4 .lang-text { font-family: 'Lora', serif; }
    
    /* 5. Cinematic */
    .font-5 h1, .font-5 h2, .font-5 h3 { font-family: 'Bodoni Moda', serif; }
    .font-5 p, .font-5 .tagline, .font-5 .lang-text { font-family: 'IBM Plex Sans', sans-serif; font-weight: 300; }
    
    /* 6. Syne + Manrope (RECOMMENDED) */
    .font-6 h1, .font-6 h2, .font-6 h3 { font-family: 'Syne', sans-serif; font-weight: 500; }
    .font-6 p, .font-6 .tagline, .font-6 .lang-text { font-family: 'Manrope', sans-serif; font-weight: 300; }
    
    /* 7. Onest */
    .font-7 h1, .font-7 h2, .font-7 h3 { font-family: 'Onest', sans-serif; font-weight: 400; }
    .font-7 p, .font-7 .tagline, .font-7 .lang-text { font-family: 'Onest', sans-serif; font-weight: 300; }
    
    /* 8. Unbounded + Bitter */
